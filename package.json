{
  "name": "browserify-zlib",
  "version": "0.1.4",
  "description": "Full zlib module for browserify",
  "keywords": [
    "zlib",
    "browserify"
  ],
  "main": "src/index.js",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "pako": "~1.0.1"
  },
  "devDependencies": {
    "brfs": "^1.0.1",
    "karma": "^1.1.0",
    "karma-browserify": "^5.0.3",
    "karma-chrome-launcher": "^1.0.1",
    "karma-firefox-launcher": "^0.1.7",
    "karma-mocha": "^1.0.1",
    "karma-mocha-own-reporter": "^1.1.2",
    "karma-phantomjs-launcher": "^1.0.0",
    "mocha": "^2.4.5",
    "phantomjs-prebuilt": "^2.1.7",
    "standard": "^7.1.1",
    "watchify": "^3.7.0"
  },
  "testling": {
    "files": "test/*.js",
    "browsers": [
      "ie/6..latest",
      "chrome/22..latest",
      "firefox/16..latest",
      "safari/latest",
      "opera/11.0..latest",
      "iphone/6",
      "ipad/6",
      "android-browser/latest"
    ]
  },
  "scripts": {
    "lint": "standard",
    "test": "npm run test:node && npm run test:browser",
    "test:browser": "karma start --single-run=true karma.conf.js",
    "test:node": "mocha test/test-*"
  },
  "author": "Devon Govett <devongovett@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/devongovett/browserify-zlib.git"
  }
}
